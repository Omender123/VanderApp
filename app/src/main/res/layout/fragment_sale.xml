<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/NestedScrollBarStyle"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context=".Activity.SaleActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="23dp"
        android:onClick="back"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:src="@drawable/ic_baseline_arrow_back_ios_24"/>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="-70dp"
            android:src="@drawable/ic_logo_text"/>

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#949494"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/report_text"
        android:fontFamily="@font/roboto_medium"
        android:layout_marginTop="10dp"
        android:textSize="18sp"
        android:textStyle="normal"
        android:textColor="#FB9595"
        android:layout_marginLeft="15dp"/>
    <View
        android:layout_width="122dp"
        android:layout_height="2dp"
        android:background="#D45B5B"
        android:layout_marginLeft="15dp"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:text="This month"
        android:layout_marginTop="@dimen/dimen20dp"
        android:layout_marginLeft="25dp"
        android:textSize="18sp"
        android:fontFamily="@font/robotomedium"
        android:textStyle="normal"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:orientation="horizontal">
        <androidx.cardview.widget.CardView
            android:layout_width="140dp"
            android:layout_height="110dp"
            android:layout_margin="@dimen/dimen10dp"
            app:cardCornerRadius="7dp"
            app:cardElevation="4dp"
            app:cardBackgroundColor="#F6F6F6"
            >
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView1"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    app:cardCornerRadius="@dimen/dimen10dp"
                    android:layout_margin="15dp"
                    android:elevation="-10dp"
                    app:cardBackgroundColor="#5E7F98">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="$"
                        android:textSize="10sp"
                        android:textColor="#FFFFFF"
                        android:fontFamily="@font/robotomedium"/>
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/gross"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/cardView1"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="10dp"
                    android:text="$234.29"
                    android:textSize="15sp"
                    android:fontFamily="@font/robotomedium"
                    android:textStyle="normal"
                    android:textColor="#445868"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/gross"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="3dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="10dp"
                    android:text="Gross sale"
                    android:textSize="12sp"
                    android:fontFamily="@font/robotomedium"
                    android:textStyle="normal"
                    android:textColor="#949EA5"/>
            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="140dp"
            android:layout_height="110dp"
            android:layout_margin="@dimen/dimen10dp"
            app:cardCornerRadius="7dp"
            app:cardElevation="4dp"
            app:cardBackgroundColor="#F6F6F6"
            >
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView2"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    app:cardCornerRadius="@dimen/dimen10dp"
                    android:layout_margin="15dp"
                    android:elevation="-10dp"
                    app:cardBackgroundColor="#DF6161">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:src="@drawable/ic_baseline_content_copy_24"/>
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/gross1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/cardView2"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="10dp"
                    android:text="$200.29"
                    android:textSize="15sp"
                    android:fontFamily="@font/robotomedium"
                    android:textStyle="normal"
                    android:textColor="#DF6161"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/gross1"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="3dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="10dp"
                    android:text="Gross sale"
                    android:textSize="12sp"
                    android:fontFamily="@font/robotomedium"
                    android:textStyle="normal"
                    android:textColor="#949EA5"/>
            </RelativeLayout>
        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:orientation="horizontal">
        <androidx.cardview.widget.CardView
            android:layout_width="140dp"
            android:layout_height="110dp"
            android:layout_margin="@dimen/dimen10dp"
            app:cardCornerRadius="7dp"
            app:cardElevation="4dp"
            app:cardBackgroundColor="#F6F6F6"

            >
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView3"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    app:cardCornerRadius="@dimen/dimen10dp"
                    android:layout_margin="15dp"
                    android:elevation="-10dp"
                    app:cardBackgroundColor="#ACDAAE">
                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_gravity="center"
                         android:src="@drawable/openbox"/>
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/gros2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/cardView3"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="10dp"
                    android:text="60"
                    android:textSize="15sp"
                    android:fontFamily="@font/robotomedium"
                    android:textStyle="normal"
                    android:textColor="#ACDAAE"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/gros2"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="3dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="10dp"
                    android:text="Sold items"
                    android:textSize="12sp"
                    android:fontFamily="@font/robotomedium"
                    android:textStyle="normal"
                    android:textColor="#949EA5"/>
            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="140dp"
            android:layout_height="110dp"
            android:layout_margin="@dimen/dimen10dp"
            app:cardCornerRadius="7dp"
            app:cardElevation="4dp"
            app:cardBackgroundColor="#F6F6F6"
            >
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView4"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    app:cardCornerRadius="@dimen/dimen10dp"
                    android:layout_margin="15dp"
                    android:elevation="-10dp"
                    app:cardBackgroundColor="#A2F0E2">
                    <ImageView
                        android:layout_width="8dp"
                        android:layout_height="9dp"
                        android:layout_gravity="center"
                        android:src="@mipmap/regular"
                        />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/gross3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/cardView4"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="10dp"
                    android:text="40"
                    android:textSize="15sp"
                    android:fontFamily="@font/robotomedium"
                    android:textStyle="normal"
                    android:textColor="#A2F0E2"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/gross3"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="3dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="10dp"
                    android:text="Order received"
                    android:textSize="12sp"
                    android:fontFamily="@font/robotomedium"
                    android:textStyle="normal"
                    android:textColor="#949EA5"/>
            </RelativeLayout>
        </androidx.cardview.widget.CardView>

    </LinearLayout>
</LinearLayout>
